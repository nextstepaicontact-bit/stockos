        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "x-tenant-id",
              "value": "11111111-1111-1111-1111-111111111111"
            }
          ]
        },
        "options": {}
      },
      "id": "a1b2c3d4-0027-0027-0027-000000000027",
