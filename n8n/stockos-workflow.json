        "headerParameters": {
          "parameters": [
            {
              "name": "x-tenant-id",
              "value": "={{ $json.tenant_id }}"
            },
            {
              "name": "x-warehouse-id",
              "value": "={{ $json.warehouse_id }}"
            },
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
