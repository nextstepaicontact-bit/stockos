      "name": "Daily Report (8 AM)",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [250, 860]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "http://localhost:3000/api/v1/analytics/daily-summary",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "x-tenant-id",
              "value": "11111111-1111-1111-1111-111111111111"
            }
          ]
        },
        "options": {}
      },
      "id": "a1b2c3d4-0021-0021-0021-000000000021",
      "name": "Get Daily Summary",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [480, 860]
    },
    {
      "parameters": {
        "select": "channel",
        "channelId": {
          "__rl": true,
          "value": "inventory-reports",
          "mode": "name"
        },
        "text": "=:bar_chart: *Daily Inventory Report - {{ new Date().toISOString().split('T')[0] }}*\n\n*Movements:*\n- Inbound: {{ $json.inbound_count }} ({{ $json.inbound_units }} units)\n- Outbound: {{ $json.outbound_count }} ({{ $json.outbound_units }} units)\n- Transfers: {{ $json.transfer_count }}\n\n*Stock Status:*\n- Total SKUs: {{ $json.total_skus }}\n- Low Stock Items: {{ $json.low_stock_count }}\n- Out of Stock: {{ $json.out_of_stock_count }}\n\n*Orders:*\n- New: {{ $json.new_orders }}\n- Shipped: {{ $json.shipped_orders }}\n- Pending: {{ $json.pending_orders }}",
        "otherOptions": {
          "includeLinkToWorkflow": false
        }
      },
      "id": "a1b2c3d4-0022-0022-0022-000000000022",
      "name": "Slack - Daily Report",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 2.2,
      "position": [720, 860]
    },
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "stockos-receiving",
